<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
<container>
    <row class="hero">
    <col>
    <div class="hero-intro">
                <h2>Meet our Musicians</h2><br>
    <ul id="musicians">{{#each musicians}}
        <li class="musiciansLI">
            <ul>
                

                        <!-- Trigger the modal with a button -->
            <a href="#myModal{{this.username}}" data-toggle="modal" class="open-model">{{this.username}}</a>

            <!-- Modal -->
            <div class="modal fade" id="myModal{{this.username}}" tabindex="1" role="dialog" aria-labelledby="myProfile" aria-hidden="true">
                <div class="modal-dialog">
    
      <!-- Modal content-->
                     <div class="modal-content">
                            <div class="modal-header">
                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                                <h4 class="modal-title"></h4>
                            </div>
                            <div class="modal-body">
                                <h4 class="modal-title" id="name">{{this.username}}</h4>
                                <p><img src ="/uploads/{{this.image}}" id = "profil-image" width="80" height="80"><br>
                                <label id="bio_intro">About me : {{this.bio}} </label><br>
                                <ul>Musician Submissions
                                <li><a href ="#">link to music</a></p>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
                            </div>
                        </div>
      
                </div>
            </div> 
            </ul>
        </li>
        {{/each}}
    </ul>

            
    </div>
    </col>
    </row>
</container>
<script>
$(document).on("click", ".open-model", function () {
     var myBookId = $(this).data('id');
     $(".modal-body #profil-image").val( myBookId );
     // As pointed out in comments, 
     // it is unnecessary to have to manually call the modal.
     // $('#addBookDialog').modal('show');
});
</script>